CREATE TABLE `islandora_digital_workflow_batch` (
	`batch_id` INT(11) UNSIGNED NOT NULL AUTO_INCREMENT,
	`batch_name` VARCHAR(50) NOT NULL DEFAULT '0',
	`batch_description` LONGTEXT NOT NULL,
	`item_count` INT(11) NULL DEFAULT '0',
	`uploaded_csv_file` TEXT NULL,
	`ingest_namespace` VARCHAR(20) NULL DEFAULT NULL,
	`default_type_of_resource` VARCHAR(50) NOT NULL DEFAULT '',
	`mapto_collections` VARCHAR(255) NULL DEFAULT NULL,
	`mapto_site_id_values` VARCHAR(255) NULL DEFAULT NULL COMMENT 'comma separated values',
	`default_ead_id` VARCHAR(50) NOT NULL DEFAULT '',
	`nid` INT(11) UNSIGNED NOT NULL DEFAULT '0',
	`user` VARCHAR(50) NOT NULL,
	`default_depositor` VARCHAR(100) NOT NULL DEFAULT '',
	`batch_host` VARCHAR(255) NOT NULL,
	`default_genre` VARCHAR(50) NOT NULL DEFAULT '',
	`batch_default_pub_status` VARCHAR(12) NOT NULL DEFAULT '',
	`batch_default_CR_status` VARCHAR(12) NOT NULL DEFAULT '',
	`batch_default_CR_holder` LONGTEXT NOT NULL,
	`lastmod` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`is_batch_active` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0',
	`batch_priority` VARCHAR(50) NOT NULL DEFAULT '',
	`batch_property_owner_id` INT(11) UNSIGNED NOT NULL DEFAULT '0',
	`batch_default_perm_notes` LONGTEXT NOT NULL,
	`structural_metadata_treatment` LONGTEXT NOT NULL,
	`image_editing_treatment` LONGTEXT NOT NULL,
	`blank_and_missing_treatment` LONGTEXT NOT NULL,
	`page_edge_treatment` LONGTEXT NOT NULL,
	`content_type_id` INT(5) UNSIGNED NOT NULL DEFAULT '0',
	`batch_condition_handling` LONGTEXT NOT NULL,
	`use_color_target` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0',
	`is_batch_request` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0',
	`has_file` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0',
	`batch_sequence_id` INT(11) UNSIGNED NOT NULL DEFAULT '0',
	`overwrite_if_exists` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0',
	`batch_item_type_id` INT(11) UNSIGNED NOT NULL DEFAULT '0',
	`type_id` INT(10) UNSIGNED NOT NULL DEFAULT '0',
	`new_or_existing` VARCHAR(20) NOT NULL DEFAULT '0' COMMENT 'new or existing Collection',
	`default_voyager_id` VARCHAR(50) NOT NULL DEFAULT '',
	`output_target_size` VARCHAR(50) NOT NULL DEFAULT '',
	`image_color_type_and_bitdepth` VARCHAR(50) NOT NULL DEFAULT '',
	`image_resolution` VARCHAR(50) NOT NULL DEFAULT '',
	`file_naming_scheme` VARCHAR(50) NOT NULL DEFAULT '',
	`file_type` VARCHAR(50) NOT NULL DEFAULT '',
	`batch_request_due_date` TIMESTAMP NULL DEFAULT NULL,
	`batch_requestor` VARCHAR(50) NOT NULL DEFAULT '',
	`batch_source_identifier` VARCHAR(50) NOT NULL DEFAULT '',
	`file` VARCHAR(255) NOT NULL DEFAULT '0',
	`date` DATE NOT NULL DEFAULT '0000-00-00',
	PRIMARY KEY (`batch_id`),
	UNIQUE INDEX `Index 2` (`batch_name`)
)
COLLATE='utf8_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=21
;
